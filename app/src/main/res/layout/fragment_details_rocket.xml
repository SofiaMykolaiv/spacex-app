        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/layout_specifications">

        <TextView
            android:id="@+id/header_capability"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/spacing_quarter"
            android:layout_marginEnd="@dimen/spacing_quarter"
            android:gravity="center"
            android:text="@string/details_capability"
            android:textColor="@color/white"
            app:layout_constraintEnd_toEndOf="parent"
