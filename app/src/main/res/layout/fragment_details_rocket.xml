            app:layout_constraintTop_toBottomOf="@+id/header_capability" />

        <TextView
            android:id="@+id/text_low_earth_orbit"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/spacing_quarter"
