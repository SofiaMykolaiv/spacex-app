            app:layout_constraintTop_toBottomOf="@+id/label_height" />

        <TextView
            android:id="@+id/text_diameter"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="end"
            android:textColor="@color/white"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/label_diameter"
            app:layout_constraintTop_toBottomOf="@+id/text_height"
            tools:text="3,7m" />

        <TextView
            android:id="@+id/label_weight"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/spacing_quarter"
            android:gravity="start"
            android:text="@string/details_weight"
            android:textColor="@color/white"
            app:layout_constraintEnd_toStartOf="@+id/text_weight"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/label_diameter" />

        <TextView
            android:id="@+id/text_weight"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/spacing_quarter"
            android:gravity="end"
            android:textColor="@color/white"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/label_weight"
            app:layout_constraintTop_toBottomOf="@+id/text_diameter"
            tools:text="30 146 kg" />

        <View
            android:id="@+id/divider_specifications_fairing"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginStart="@dimen/spacing_quarter"
            android:layout_marginTop="@dimen/spacing_half"
            android:layout_marginEnd="@dimen/spacing_quarter"
            android:background="@color/white"
            android:visibility="visible"
            app:layout_constraintTop_toBottomOf="@+id/label_weight" />

        <TextView
            android:id="@+id/label_fairing_height"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/spacing_quarter"
            android:layout_marginTop="@dimen/spacing_half"
            android:gravity="start"
            android:text="@string/details_fairing_height"
            android:textColor="@color/white"
            app:layout_constraintEnd_toStartOf="@+id/text_fairing_height"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/divider_specifications_fairing" />

        <TextView
            android:id="@+id/text_fairing_height"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/spacing_half"
            android:layout_marginEnd="@dimen/spacing_quarter"
            android:gravity="end"
            android:textColor="@color/white"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/label_fairing_height"
            app:layout_constraintTop_toBottomOf="@+id/divider_specifications_fairing"
            tools:text="3.5 m" />

        <TextView
            android:id="@+id/label_fairing_diameter"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/spacing_quarter"
            android:layout_marginTop="@dimen/spacing_half"
            android:gravity="start"
            android:text="@string/details_fairing_diameter"
            android:textColor="@color/white"
            app:layout_constraintEnd_toStartOf="@+id/text_fairing_diameter"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/label_fairing_height" />

        <TextView
            android:id="@+id/text_fairing_diameter"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/spacing_half"
            android:layout_marginEnd="@dimen/spacing_quarter"
            android:gravity="end"
            android:textColor="@color/white"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/label_fairing_diameter"
            app:layout_constraintTop_toBottomOf="@+id/text_fairing_height"
            tools:text="1.5 m" />
    </androidx.constraintlayout.widget.ConstraintLayout>


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/layout_capability"
